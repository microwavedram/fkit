{
  "name": "fkit",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "fKit": {
        "$className": "Folder",
        "Packages": {
          "$path": "Packages"
        },
        "Common": {
          "$path": "src/shared"
        },
        "Configuration": {
          "$className": "Configuration",
          "$path": "src/config"
        }
      }
    },

    "ServerScriptService": {
      "fKit": {
        "$className": "Folder",
        "Server": {
          "$path": "src/server"
        },
        "Resources": {
          "$className": "Folder",
          "$ignoreUnknownInstances": true
        }
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "fKit": {
          "$className": "Folder",
          "Client": {
            "$path": "src/client"
          }
        }
      }
    }
  }
}